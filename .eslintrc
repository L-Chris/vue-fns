{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint-config-airbnb-base",
    "plugin:import/typescript",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "global-require": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
